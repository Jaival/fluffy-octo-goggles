{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ .UserName }}@{{ .HostName }} {{ .Shell }} in {{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "p:default_fg",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": " ",
          "template": "{{ if .WSL }}\ue62a on {{ end }}{{ if .Icon }}{{ .Icon }}{{ end }}",
          "properties": {
            "alma": "\uf31d",
            "alpine": "\uf300",
            "aosc": "\uf301",
            "arch": "\uf303",
            "centos": "\uf304",
            "coreos": "\uf305",
            "debian": "\uf306",
            "devuan": "\uf307",
            "display_distro_name": false,
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\uf17c",
            "macos": "\uf179",
            "mageia": "\uf310",
            "manjaro": "\uf312",
            "mint": "\uf30e",
            "nixos": "\uf313",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "redhat": "\uf316",
            "rocky": "\uf32b",
            "sabayon": "\uf317",
            "slackware": "\uf319",
            "ubuntu": "\uf31b",
            "windows": "\ue62a"
          }
        },
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "p:default_fg",
          "background": "p:default_bg",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if .Name }}{{ .Name }}{{ end }}"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "p:default_fg",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if .SSHSession }}\udb80\udf18 {{ end }}{{ if .Root }}{{ if .UserName }}<#ff0055>{{ .UserName }}</>{{ end }}{{ else }}{{ .UserName }}{{ end }}{{ if .HostName }}{{ if .UserName }}@{{ end }}{{ .HostName }}{{ end }}"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "p:default_fg",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if .Path }}{{ .Path }}{{ end }}",
          "properties": {
            "folder_icon": "...",
            "folder_separator_icon": "/",
            "hide_root_location": false,
            "home_icon": "\uf015 ",
            "max_depth": 3,
            "mixed_threshold": 5,
            "style": "agnoster_short",
            "windows_registry_icon": "\ue0b1"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#43CCEA",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#b388ff{{ end }}",
            "{{ if gt .Behind 0 }}#b388ff{{ end }}"
          ],
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ url .UpstreamIcon .UpstreamURL }}{{ if .UpstreamIcon }}{{ end }}{{ if .HEAD }} {{ .HEAD }}{{ end }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }}",
          "properties": {
            "azure_devops_icon": "\ufd03",
            "bitbucket_icon": "\uf171",
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone_icon": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "branch_max_length": 25,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_bare_info": true,
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": false,
            "git_icon": "\ue5fb",
            "github_icon": "\uf408",
            "gitlab_icon": "\uf296",
            "merge_icon": "\ue727 ",
            "native_fallback": false,
            "no_commits_icon": "\udb80\udc95 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 ",
            "truncate_symbol": "",
            "upstream_icons": {
              "git.wanadev.org": "\uf296",
              "git2.wanadev.org": "\uf296"
            }
          }
        }
      ],
      "overflow": "break"
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "terraform",
          "style": "diamond",
          "foreground": "#844fba",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ufa08 {{ if .WorkspaceName }}{{ .WorkspaceName }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#ffe263",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue235 {{ if .Full }}{{ .Full }}{{ end }}{{ if .Venv }} ({{ .Venv }}){{ end }}",
          "properties": {
            "display_default": true,
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true,
            "home_enabled": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#79d4fd",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\udb81\udfd3 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false,
            "parse_mod_file": true
          }
        },
        {
          "type": "rust",
          "style": "diamond",
          "foreground": "#db1010",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue7a8 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#cc342d",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue791 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#026e00",
          "background": "background",
          "trailing_diamond": " ",
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true,
            "npm_icon": "<#FE4A49>\ue71e npm</>",
            "yarn_icon": "<#44FFD2>\uf487 yarn</>"
          }
        },
        {
          "type": "cmake",
          "style": "diamond",
          "foreground": "#5fc004",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue61e {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "angular",
          "style": "diamond",
          "foreground": "#dd0031",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue753 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "foreground": "#5c2d91",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue77f {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "flutter",
          "style": "diamond",
          "foreground": "#0468d7",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue64c {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "haskell",
          "style": "diamond",
          "foreground": "#5e5086",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue61f {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "foreground": "#cb3c33",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue624 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "kotlin",
          "style": "diamond",
          "foreground": "#7f52ff",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue634 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "lua",
          "style": "diamond",
          "foreground": "#00007c",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue620 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "perl",
          "style": "diamond",
          "foreground": "#0073a1",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue769 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "php",
          "style": "diamond",
          "foreground": "#7377ad",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue73d {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "r",
          "style": "diamond",
          "foreground": "#2266b8",
          "background": "background",
          "trailing_diamond": " ",
          "template": "R {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "swift",
          "style": "diamond",
          "foreground": "#ef6354",
          "background": "background",
          "trailing_diamond": " ",
          "template": "\ue755 {{ if .Full }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": false
          }
        },
        {
          "type": "project",
          "style": "diamond",
          "foreground": "#5485a3",
          "background": "background",
          "trailing_diamond": " ",
          "template": "{{ if or .Version .Name }}\uf487{{ end }}{{ if .Version }} {{ .Version }}{{ end }}{{ if .Name }} {{ .Name }}{{ end }}"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#eb3434",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if gt .Code 0 }}\uf00d {{ if .Meaning }}{{ .Meaning }}{{ end }}{{ else }}\uf00c {{ end }}",
          "properties": {
            "always_enabled": false
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#727272",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf252 {{ if .FormattedMs }} {{ .FormattedMs }}{{ end }}",
          "properties": {
            "style": "roundrock",
            "threshold": 2000
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#727272",
          "background": "p:default_bg",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\udb82\udd54 {{ if .CurrentDate }}{{ .CurrentDate | date .Format }}{{ end }}",
          "properties": {
            "style": "mixed"
          }
        }
      ],
      "overflow": "break"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#1fe64a",
          "trailing_diamond": " ",
          "template": "âžœ"
        }
      ],
      "newline": true
    }
  ],
  "tooltips": [
    {
      "type": "kubectl",
      "tips": [
        "k",
        "kubectl",
        "kind",
        "minikube",
        "helm",
        "kubens",
        "kcx",
        "kubectx",
        "kcx",
        "oc",
        "istioctl",
        "kogito",
        "k9s",
        "helmfile",
        "flux",
        "fluxctl",
        "stern",
        "kubeseal",
        "skaffold",
        "kubent",
        "kubecolor"
      ],
      "style": "diamond",
      "foreground": "#316ce6",
      "background": "background",
      "trailing_diamond": " ",
      "template": "\udb82\udc33 {{ if .Context }}{{ .Context }}{{ end }}{{ if .Cluster }}{{ if ne .Cluster .Context }}{{ if .Context }} :: {{ end }}{{ .Cluster }}{{ end }}{{ end }}{{ if or .Context .Cluster }} :: {{ end }}{{ if .Namespace }}{{ .Namespace }}{{ else }}default{{ end }}{{ if .User }}{{ if ne .User .Cluster }}{{ if or .Context .Cluster}} :: {{ end }}{{ .User }}{{ end }}{{ end }}",
      "properties": {
        "display_error": false,
        "parse_kubeconfig": false
      }
    },
    {
      "type": "aws",
      "tips": [
        "aws",
        "awless"
      ],
      "style": "diamond",
      "foreground": "#ec912d",
      "background": "background",
      "trailing_diamond": " ",
      "template": "\udb83\ude0f {{ if .Profile }}{{.Profile}}{{ end }}{{ if .Region }}{{ if .Profile }}@{{ end }}{{ .Region }}{{ end }}",
      "properties": {
        "display_default": false
      }
    },
    {
      "type": "az",
      "tips": [
        "az"
      ],
      "style": "diamond",
      "foreground": "#0078d4",
      "background": "background",
      "trailing_diamond": " ",
      "template": "\ufd03 {{ if .User.Name }}{{ .User.Name }}{{ end }}{{ if .EnvironmentName}}{{ if .User.Name }}@{{ end }}{{ .EnvironmentName }}{{ end }}",
      "properties": {
        "source": "first_match"
      }
    },
    {
      "type": "gcp",
      "tips": [
        "gcloud",
        "gcs",
        "gsutil"
      ],
      "style": "diamond",
      "foreground": "#1a73e8",
      "background": "background",
      "trailing_diamond": " ",
      "template": "\ue7b2 {{ if .Account }}{{ .Account }}{{ end }}{{ if .Region }}{{ if .Account }}@{{ end }}{{ .Region }}{{ end }}{{ if .Project }}{{ if or .Account .Region }} :: {{ end }}{{ .Project }}{{ end }}"
    },
    {
      "type": "docker",
      "tips": [
        "d",
        "docker",
        "dc",
        "docker-compose"
      ],
      "style": "diamond",
      "foreground": "#2496ed",
      "background": "background",
      "trailing_diamond": " ",
      "template": "\ue7b0 {{ if .Context }}{{ .Context }}{{ end }}"
    }
  ],
  "palette": {
    "default_bg": "#29315A",
    "default_fg": "#0077ff"
  }
}
