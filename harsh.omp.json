{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "version": 2,
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b0",
          "foreground": "#000000",
          "background": "#8ac926",
          "template": " \uf489 {{ .Name }} "
        },
        {
          "type": "git",
          "style": "diamond",
          "background": "#FFFB38",
          "foreground": "#011627",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_user": true
          },
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#023047",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "folder_icon": "\uF4C1",
            "style": "full",
            "mapped_locations": {
              "~/.config": "",
              "~/config-files": "",
              "~/GitHub": "\uF09B"
            }
          },
          "template": " {{ .Path }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#627264",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }} {{ end }}{{ .UserName }} "
        },
        {
          "type": "lua",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#415A77",
          "template": "  {{ .Full }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#3C873A",
          "background": "#303030",
          "template": "  {{ .Full }} "
        },
        {
          "type": "docker",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#000000",
          "background": "#0B59E7",
          "template": "  {{ .Context }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#020202",
          "background": "#2EC4B6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "3:04 PM"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "spotify",
          "style": "plain",
          "foreground": "#1BD760",
          "template": "\u2502\u2500( {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} )"
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#8ac926",
          "template": "\u2570\u2500"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#219ebc",
          "template": "<#8ac926>\u2500</>(\uF0E7)"
        },
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "#FFD664",
          "template": "<#8ac926>\u2500</>(\uf019: {{.Version}})"
        },
        {
          "type": "npm",
          "style": "plain",
          "foreground": "#FFD664",
          "template": "<#8ac926>\u2500</>( {{ .Full }})"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "template": "\u276f"
        }
      ]
    }
  ]
}
